{
  "description": "An empty TiddlyWiki containing \"blc\", a TiddlyWiki plugin for kanban like organization in the style of Trello using boards, lists, and cards.",
  "plugins": [
    "blc"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "languages": [
  ],
  "build": {
    "blc-plugin": [
      "--rendertiddler", "$:/plugins/reidgould/blc", "tiddlywiki-blc-plugin.tid", "text/plain", "$:/core/templates/exporters/TidFile", "exportFilter", "[<currentTiddler>]"
    ],
    "empty-with-blc": [
      "--rendertiddler","$:/core/save/all","TiddlyWiki With BLC.html","text/plain"
    ],
    "empty-with-blc-downloadButton": [
      "--rendertiddler","$:/plugins/reidgould/blc/download-empty-with-blc","TiddlyWiki With BLC.html","text/plain"
    ],
    "meta": [
      "--savetiddler","$:/plugins/reidgould/blc/readme","ReadMe.md",
      "--savetiddler","$:/plugins/reidgould/blc/license","license.txt"
    ]
  },
  "config": {
    "default-tiddler-location": "./newTiddlers",
    "retain-original-tiddler-path": false
  }
}
